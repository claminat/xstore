<% layout('layout') -%>
    <div class="jumbotron">
        <h1>
            <%= title %>
        </h1>
        <h2>Variable</h2>
        <p>
            <%= tagline %>
        </p>
        <h2>Loop</h2>

        <ul>
            <% drinks.forEach(function(drink) { %>
                <li>
                    <%= drink.name %> -
                        <%= drink.drunkness %>
                </li>
                <% }); %>
        </ul>
        <h1>This is great</h1> index
        <p>Welcome to templating using EJS</p>
    </div>